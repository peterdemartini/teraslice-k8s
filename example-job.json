{
    "name": "example-data-generator-job",
    "lifecycle": "once",
    "workers": 3,
    "assets": [
        "example"
    ],
    "operations": [
        {
            "_op": "elasticsearch_data_generator",
            "size": 1000000
        },
        {
            "_op": "example-op"
        },
        {
            "_op": "elasticsearch_index_selector",
            "index": "example-teraslice-k8s-test",
            "type": "events"
        },
        {
            "_op": "elasticsearch_bulk",
            "size": 1000
        }
    ]
}
